<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:layout_editor_absoluteX="0dp"
    tools:layout_editor_absoluteY="86dp">

    <View
        android:id="@+id/emailBox6"

        android:layout_width="344dp"
        android:layout_height="49dp"
        android:layout_alignTop="@+id/birthday"
        android:layout_alignBottom="@+id/birthday"
        android:layout_marginTop="40dp"
        android:background="@drawable/rectangle_4_shape"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/emailBox5" />

    <Spinner
        android:id="@+id/location"
        android:contentDescription="good"
        android:layout_width="313dp"
        android:layout_height="19dp"
        android:layout_alignEnd="@+id/emailBox"
        android:layout_marginBottom="28dp"
        android:ems="10"
        android:hint="Email"
        android:inputType="textEmailAddress"
        app:layout_constraintBottom_toTopOf="@+id/birthday"
        app:layout_constraintEnd_toEndOf="@+id/emailBox5"
        app:layout_constraintHorizontal_bias="0.458"
        app:layout_constraintStart_toStartOf="@+id/emailBox5" />


    <EditText
        android:id="@+id/birthday"

        android:layout_width="320dp"
        android:layout_height="59dp"
        android:layout_alignEnd="@+id/emailBox"
        android:layout_marginBottom="52dp"
        android:ems="10"
        android:hint="Birthday (dd/mm/yy)"
        android:inputType="textEmailAddress"
        app:layout_constraintBottom_toTopOf="@+id/signup1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent" />

    <View
        android:id="@+id/emailBox5"
        android:layout_width="344dp"
        android:layout_height="49dp"
        android:layout_alignTop="@+id/birthday"
        android:layout_alignBottom="@+id/birthday"
        android:background="@drawable/rectangle_4_shape"
        app:layout_constraintBottom_toTopOf="@+id/birthday"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/emailBox4" />



    <View
        android:id="@+id/emailBox4"
        android:layout_width="344dp"
        android:layout_height="49dp"
        android:layout_alignTop="@+id/birthday"
        android:layout_alignBottom="@+id/birthday"
        android:background="@drawable/rectangle_4_shape"
        app:layout_constraintBottom_toTopOf="@+id/location"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.507"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rectangle_1434"
        app:layout_constraintVertical_bias="0.781" />

    <EditText
        android:id="@+id/idNum"

        android:layout_width="320dp"
        android:layout_height="59dp"
        android:ems="10"
        android:hint="ID Number"
        android:inputType="number"
        app:layout_constraintBottom_toBottomOf="@+id/emailBox4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <View
        android:id="@+id/rectangle_1434"
        android:layout_width="442dp"
        android:layout_height="88dp"
        android:background="@drawable/blue"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/rectangle_1434"
        android:layout_marginBottom="660dp"
        android:text="together"
        android:textColor="#Fff6ea"
        android:textSize="40dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/logo_png__1__2" />

    <ImageView
        android:id="@+id/logo_png__1__2"
        android:layout_width="126dp"
        android:layout_height="123dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="-26dp"
        android:contentDescription="logo_png__1__2"
        android:src="@drawable/logofinal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/rectangle_1434"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <ImageButton
        android:id="@+id/signup1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="216dp"
        android:background="@color/white"
        android:contentDescription="good"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"

        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/elderly" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        app:layout_constraintBottom_toTopOf="@+id/idNum"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.08"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/elderlysignup" />


</androidx.constraintlayout.widget.ConstraintLayout>